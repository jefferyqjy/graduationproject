<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pro.entity.FoodAppoint" table="t_foodappoint">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="increment"/>
        </id>
        <property access="field" column="t_custname" length="200" name="custname"/>
        <property access="field" column="t_menuid" length="200" name="menuid"/>
        <property access="field" column="t_createtime" length="200" name="createtime"/>
        <property access="field" column="t_count" length="200" name="count"/>
        <property access="field" column="t_status" length="200" name="status"/>
    </class>
</hibernate-mapping>
