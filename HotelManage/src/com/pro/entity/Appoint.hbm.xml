<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pro.entity.Appoint" table="t_appoint">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="native"/>
        </id>
        <property access="field" column="t_custname" length="200" name="custname"/>
        <property access="field" column="t_identityid" length="200" name="identityid"/>
        <property access="field" column="t_remarks" length="200" name="remarks"/>
        <property access="field" column="t_begintime" length="200" name="begintime"/>
        <property access="field" column="t_endtime" length="200" name="endtime"/>
        <property access="field" column="t_status" length="200" name="status"/>
        <property access="field" column="t_personnum" length="200" name="personnum"/>
        <property access="field" column="t_custuserid" length="200" name="custuserid"/>
        <property access="field" column="t_phone" length="200" name="phone"/>
    </class>
</hibernate-mapping>
